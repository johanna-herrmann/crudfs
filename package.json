{
  "name": "crudfs",
  "version": "0.1.0",
  "description": "REST file storage with CRUD based permissions on directory-level, written in Typescript.",
  "contributors": [
    {
      "name": "johanna-herrmann",
      "email": "johanna.herrmann@mailbox.org"
    }
  ],
  "private": true,
  "main": "built/lib/index.js",
  "types": "built/lib/index.d.ts",
  "author": "Johanna Herrmann",
  "scripts": {
    "inst": "npm install",
    "audit": "better-npm-audit audit",
    "lint": "eslint **/*.ts",
    "test": "tsc --noEmit --sourceMap && jest --runInBand --verbose --detectOpenHandles --forceExit",
    "qa": "npm run lint && npm run test",
    "build": "tsc --build --clean && tsc && tsc-alias"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johanna-herrmann/crudfs.git"
  },
  "keywords": [
    "rest",
    "crud",
    "file",
    "storage"
  ],
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.13.0",
    "@types/jest": "29.5.13",
    "@types/mock-fs": "4.13.4",
    "@types/node": "22.8.7",
    "@typescript-eslint/eslint-plugin": "8.10.0",
    "@typescript-eslint/parser": "8.10.0",
    "aws-sdk-client-mock": "4.1.0",
    "better-npm-audit": "3.11.0",
    "esbuild-jest": "0.5.0",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "mock-fs": "5.4.0",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.10",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.678.0"
  }
}
